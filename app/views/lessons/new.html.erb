<%= form_for @lesson, url: {action: "create"} do |f|%>
    <%= f.label :image_url%>
    <%= f.text_field :image_url%>

    <%= f.label :title%>
    <%= f.text_field :title %>

    <%= f.label :duration %>
    <%= f.text_field :duration %>

    <%= f.label :cost %>
    <%= f.text_field :cost %>

    <%= f.label :description%>
    <%= f.text_field :description%>

    <%= f.submit %>



<% end %>
